{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 3,
  "settings": {
    "appearanceTools": true,
    "border": {},
    "color": {
      "defaultPalette": false,
      "custom": false,
      "defaultGradients": false,
      "customGradient": false,
      "defaultDuotone": false,
      "palette": [
        {
          "color": "#FFCD00",
          "name": "Brand",
          "slug": "brand"
        },
        {
          "color": "#ffffff",
          "name": "White",
          "slug": "white"
        },
        {
          "color": "#FAF8F2",
          "name": "Light",
          "slug": "light"
        },
        {
          "color": "#151515",
          "name": "Dark",
          "slug": "dark"
        },
        {
          "color": "#FFBBA0",
          "name": "Peach",
          "slug": "peach"
        },
        {
          "color": "#844DD6",
          "name": "Purple",
          "slug": "purple"
        },
        {
          "color": "#612DAF",
          "name": "Aubergine",
          "slug": "aubergine"
        },
        {
          "color": "#A782FF",
          "name": "Lilly",
          "slug": "lilly"
        },
        {
          "color": "#FFE05F",
          "name": "Sunflower",
          "slug": "sunflower"
        },
        {
          "color": "#28A745",
          "name": "Success",
          "slug": "success"
        },
        {
          "color": "#17A2B8",
          "name": "Info",
          "slug": "info"
        },
        {
          "color": "#DC3545",
          "name": "Failure",
          "slug": "failure"
        }
      ],
      "gradients": [
        {
          "gradient": "linear-gradient(to right, var(--wp--preset--color--white), var(--wp--preset--color--brand))",
          "name": "White to Brand",
          "slug": "white-to-brand"
        }
      ]
    },
    "custom": {
      "lineHeight": {
        "xs": "1.1",
        "sm": "1.2",
        "md": "1.4",
        "lg": "1.5"
      },
      "breakpoints": {
        "sm": "640px",
        "md": "768px",
        "lg": "1024px",
        "xl": "1280px",
        "2xl": "1536px"
      },
      "spacingSizes": {
        "small": "clamp(.5rem, 3.7vw + -0.05rem,  1rem)",
        "medium": "clamp(1.5rem, 3.7vw + -0.05rem,  3rem )",
        "large": "clamp(2rem, 3.7vw + -0.05rem,  4rem )"
      }
    },
    "dimensions": {
      "minHeight": true
    },
    "layout": {
      "contentSize": "min(800px, 80vw)",
      "wideSize": "min(1200px, 95vw)"
    },
    "position": {},
    "shadow": {},
    "spacing": {
      "blockGap": null,
      "margin": true,
      "padding": true,
      "defaultSpacingSizes": false,
      "spacingSizes": [
        {
          "name": "Fluid small",
          "size": "var:custom|spacing-sizes|small",
          "slug": "fluid-small"
        },
        {
          "name": "Fluid medium",
          "size": "var:custom|spacing-sizes|medium",
          "slug": "fluid-medium"
        },
        {
          "name": "Fluid large",
          "size": "var:custom|spacing-sizes|large",
          "slug": "fluid-large"
        }
      ]
    },
    "typography": {
      "fluid": true,
      "lineHeight": false,
      "letterSpacing": false,
      "fontFamilies": [
        {
          "fontFace": [
            {
              "fontFamily": "Albert Sans",
              "fontStretch": "normal",
              "fontStyle": "normal",
              "fontWeight": "100 200 300 400 500 600 700 800 900",
              "src": ["file:./assets/fonts/AlbertSans-VariableFont_wght.ttf"]
            },
            {
              "fontFamily": "Albert Sans",
              "fontStretch": "normal",
              "fontStyle": "italic",
              "fontWeight": "100 200 300 400 500 600 700 800 900",
              "src": [
                "file:./assets/fonts/AlbertSans-Italic-VariableFont_wght.ttf"
              ]
            }
          ],
          "fontFamily": "\"Albert Sans\", sans-serif",
          "name": "Albert Sans",
          "slug": "albert-sans"
        },
        {
          "fontFace": [
            {
              "fontFamily": "Molitor",
              "fontStretch": "normal",
              "fontStyle": "normal",
              "fontWeight": "400",
              "src": ["file:./assets/fonts/MolitorDisplayRegular.woff"]
            },
            {
              "fontFamily": "Molitor",
              "fontStretch": "normal",
              "fontStyle": "italic",
              "fontWeight": "400",
              "src": ["file:./assets/fonts/MolitorDisplayItalic.woff"]
            },
            {
              "fontFamily": "Molitor",
              "fontStretch": "normal",
              "fontStyle": "normal",
              "fontWeight": "700",
              "src": ["file:./assets/fonts/MolitorDisplayBold.woff"]
            },
            {
              "fontFamily": "Molitor",
              "fontStretch": "normal",
              "fontStyle": "italic",
              "fontWeight": "700",
              "src": ["file:./assets/fonts/MolitorDisplayBoldItalic.woff"]
            }
          ],
          "fontFamily": "\"Molitor\", sans-serif",
          "name": "Molitor",
          "slug": "molitor"
        }
      ],
      "defaultFontSizes": false,
      "fontSizes": [
        {
          "name": "Paragraph",
          "slug": "paragraph",
          "size": "20px",
          "fluid": {
            "min": "18px",
            "max": "20px"
          }
        },
        {
          "name": "H1",
          "slug": "heading-1",
          "size": "84px"
        },
        {
          "name": "H2",
          "slug": "heading-2",
          "size": "56px"
        },
        {
          "name": "H3",
          "slug": "heading-3",
          "size": "40px"
        },
        {
          "name": "H4",
          "slug": "heading-4",
          "size": "32px"
        },
        {
          "name": "H5",
          "slug": "heading-5",
          "size": "24px"
        },
        {
          "name": "Ingress",
          "slug": "ingress",
          "size": "28px",
          "fluid": {
            "min": "20px",
            "max": "28px"
          }
        },
        {
          "fluid": false,
          "name": "Mini",
          "slug": "mini",
          "size": "16px"
        },
        {
          "name": "Maxi",
          "slug": "maxi",
          "size": "140px"
        }
      ]
    },
    "useRootPaddingAwareAlignments": false,
    "blocks": {
      "core/pullquote": {
        "border": {
          "color": true,
          "radius": true,
          "style": true,
          "width": true
        }
      },
      "core/image": {
        "lightbox": {
          "enabled": true,
          "allowEditing": true
        }
      }
    }
  },
  "styles": {
    "typography": {
      "fontFamily": "Albert Sans"
    },
    "color": {
      "text": "var:preset|color|dark",
      "background": "var:preset|color|light"
    },
    "blocks": {
      "core/paragraph": {
        "typography": {
          "lineHeight": "var:custom|line-height|md",
          "fontSize": "var:preset|font-size|paragraph"
        }
      },
      "core/heading": {
        "typography": {
          "fontSize": "var:preset|font-size|heading-2"
        }
      },
      "core/group": {
        "spacing": {
          "padding": {
            "top": "var:custom|spacing-sizes|medium",
            "bottom": "var:custom|spacing-sizes|medium"
          }
        }
      }
    },
    "elements": {
      "heading": {
        "typography": {
          "fontFamily": "Molitor"
        }
      },
      "link": {
        "color": {
          "text": "var:preset|color|brand"
        }
      }
    }
  },
  "customTemplates": [],
  "templateParts": [],
  "patterns": []
}
