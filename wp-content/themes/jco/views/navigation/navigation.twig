{% block navbar_above %}
	{% if customizer('navigation','topbar') %}
		{% include 'navigation/topbar.twig' %}
	{% endif %}
{% endblock %}

	<nav id="jcore-nav" data-jsticky>
    	<button class="hamburger" data-jtoggle="#jcore-menu" data-class="menu-open">
    	    {% include "images/hamburger.twig" %}
    	</button>
				<div class="brand">
					{% include 'images/navbar-brand.twig' %}
				</div>
				{% include "navigation/menu.twig" with {items: menu.primary.get_items, id: "jcore-menu", level: 0} only %}
	</nav>
